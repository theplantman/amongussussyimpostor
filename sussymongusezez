--whitelist--
local hwidgobrr = {
    "54A913B4-270F-4E4C-9B1D-90805596195B",
    "70238B58-E7FD-456E-A583-348730D6CBAE"
}
local hwidcheck = game:GetService("RbxAnalyticsService"):GetClientId()
for i,v in pairs(hwidgobrr) do
    if v == hwidcheck then
        print("whitelisted")
    elseif v ~= hwidcheck then
        game:GetService("Players").LocalPlayer:Kick("The Plant Man#8610 Says No Whitelist")
    end
end

--blacklist--
